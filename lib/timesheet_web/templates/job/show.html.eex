<h1>Show Job</h1>

<ul>

  <li>
    <strong>Job code:</strong>
    <%= @job.job_code %>
  </li>

  <li>
    <strong>Hours:</strong>
    <%= @job.hours %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @job.name %>
  </li>

  <li>
    <strong>Desc:</strong>
    <%= @job.desc %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.job_path(@conn, :edit, @job) %></span>
<span><%= link "Back", to: Routes.job_path(@conn, :index) %></span>
